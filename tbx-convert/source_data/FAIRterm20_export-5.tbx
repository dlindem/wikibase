            <term>sauvegarde de justice</term>
            <fairterm:identifier>590_fr_213</fairterm:identifier>
            <fairterm:usage>preferred term</fairterm:usage>
            <min:partOfSpeech>NOUN</min:partOfSpeech>
            <basic:grammaticalGender>Fem</basic:grammaticalGender>
            <basic:grammaticalNumber>Sing</basic:grammaticalNumber>
            <basic:termType>complex term</basic:termType>
            <descripGrp>
              <basic:context>Les actes passés pendant une sauvegarde de justice peuvent être remise en cause s'ils portent préjudice aux intérêts de la personne.</basic:context>
              <basic:externalCrossReference>Les actes passés pendant une sauvegarde de justice peuvent être remise en cause s'ils portent préjudice aux intérêts de la personne.</basic:externalCrossReference>
