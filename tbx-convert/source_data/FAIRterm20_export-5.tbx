            <fairterm:usage>admitted term</fairterm:usage>
            <min:partOfSpeech>NOUN</min:partOfSpeech>
            <basic:grammaticalGender>Fem</basic:grammaticalGender>
            <basic:grammaticalNumber>Sing</basic:grammaticalNumber>
            <basic:termType>complex term</basic:termType>
            <descripGrp>
              <basic:context>Dans le cas contraire, la sanction pour infraction dolosive peut quand meme etre attenuee.</basic:context>
              <basic:externalCrossReference>Dans le cas contraire, la sanction pour infraction dolosive peut quand meme etre attenuee.</basic:externalCrossReference>
              <basic:source>Commission européenne </basic:source>
            </descripGrp>
            <fairterm:register>formel</fairterm:register>
            <fairterm:collocation>commettre une infraction dolosive</fairterm:collocation>
